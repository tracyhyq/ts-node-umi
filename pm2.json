{
  "apps": [
    {
      "name": "pangoo-starter",
      "script": "src/server/index.js",
      "env": {
        "NODE_ENV": "development"
      },
      "env_test": {
        "NODE_ENV": "test"
      },
      "env_production": {
        "NODE_ENV": "production"
      },
      "out_file": "/dev/null",
      "error_file": "/dev/null",
      "exec_interpreter": "node",
      "exec_mode": "cluster_mode",
      "instances": 0,
      "log_date_format": "YYYY-MM-DD HH:mm:ss|SSS Z",
      "max_memory_restart": "700M",
      "merge_logs": true,
      "watch": false
    }
  ]
}
