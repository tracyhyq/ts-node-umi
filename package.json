{
  "name": "pangoo-umi-ssr",
  "version": "1.0.0",
  "description": "React16.*，Mobx，nodejs，Typescript, umijs 前端脚手架项目",
  "scripts": {
    "dev": "cross-env UMI_ENV=development UMI_UI=none NODE_ENV=development concurrently \"umi dev\" \"nodemon -w 'src/server' src/server/index.js\"",
    "debug": "npm run build:client && node src/server/index.js",
    "build:client:prod": "cross-env NODE_ENV=production RM_TMPDIR=none UMI_ENV=production umi build",
    "build:client:test": "cross-env NODE_ENV=test RM_TMPDIR=none COMPRESS=none UMI_ENV=test umi build ",
    "build:client:dev": "cross-env NODE_ENV=development RM_TMPDIR=none COMPRESS=none UMI_ENV=development umi build ",
    "build:server": " tsc -v && tsc -p src/server",
    "build:server:watch": " tsc -v && tsc -p src/server --watch",
    "lint:ts": "tslint -p './tsconfig.json' 'src/**/*.{ts,tsx}' -e 'src/@types/**'",
    "lint:css": "stylelint 'src/**/*.{css,less}'",
    "lint": "npm run lint:ts && npm run lint:css",
    "precommit": "pretty-quick --staged && npm run lint:css && npm run lint:ts"
  },
  "engines": {
    "node": ">8.0.0"
  },
  "_moduleAliases": {
    "@src": "src",
    "@root": ".",
    "@server": "src/server",
    "@I": "src/interfaces",
    "@serverUtils": "src/server/utils"
  },
  "dependencies": {
    "@fe/pgn-logger": "^1.0.25",
    "@fe/pgn-response-fomatter": "^1.0.25",
    "@types/classnames": "^2.2.9",
    "@types/koa-compress": "^2.0.9",
    "@types/node": "^13.1.6",
    "antd": "^3.22.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "config": "^3.2.4",
    "cross-env": "^5.2.0",
    "int64-convert": "^0.1.2",
    "js-cookie": "^2.2.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.1",
    "koa-compress": "^3.0.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "less": "^3.10.3",
    "lodash": "^4.17.15",
    "mobx": "^5.15.2",
    "mobx-react": "^6.1.5",
    "module-alias": "^2.2.1",
    "moment-timezone": "^0.5.26",
    "pm2": "^3.5.1",
    "react": "^16.8.6",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "umi-request": "^1.2.4",
    "umi-server": "^1.0.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.9.0",
    "umi-plugin-react": "^1.8.0",
    "umi-types": "^0.3.0",
    "@types/config": "0.0.36",
    "@types/js-cookie": "^2.2.2",
    "@types/koa": "^2.7.0",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.0.42",
    "@types/moment-timezone": "^0.5.12",
    "@types/react-document-title": "^2.0.3",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.1",
    "typescript": "^3.7.4",
    "umi-plugin-gh-pages": "^0.1.0",
    "husky": "^4.0.7",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "tslint": "^5.20.1",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
